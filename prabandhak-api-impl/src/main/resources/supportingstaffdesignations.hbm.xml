<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.samarthsoft.prabandhak.entities.SupportingStaffDesignations" table="SUPPORTING_STAFF_DESIGNATIONS">
		<id name="name" type="java.lang.String" column="DESIGNATION">
			<generator class="assigned" />
		</id>
		<property name="hasAssociatedLoginInformation">
			<column name="ASSOCIATE_LOGIN_INFORMATION" />
		</property>
	</class>
</hibernate-mapping>